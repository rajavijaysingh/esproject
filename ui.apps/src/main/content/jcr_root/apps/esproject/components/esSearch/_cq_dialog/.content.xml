<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Elastic Search"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Component Mapping"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
											<facet
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldDescription="Please provide facets"
												fieldLabel="Facets"
												required="{Boolean}true"
												name="./esFacets"/>												
											<componetMapping
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
												composite="{Boolean}true"
												fieldLabel="Component Properties and Search Fields Mapping">
												<field
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/container"
													name="./esxJComponentMappingList">
													<items jcr:primaryType="nt:unstructured">
														<column
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/container">
															<items jcr:primaryType="nt:unstructured">
																	<mapping
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																		fieldDescription="Please provide mapping"
																		fieldLabel="Component Mapping"
																		name="./esMapping"/>
																				
															</items>
														</column>
													 </items>
												</field>
											</componetMapping>											   
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
					<page
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Page Mapping"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">											
											<pageMapping
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
												composite="{Boolean}true"
												fieldLabel="Page Properties and Search Fields Mapping">
												<field
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/container"
													name="./esxJPageMappingList">
													<items jcr:primaryType="nt:unstructured">
														<column
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/container">
															<items jcr:primaryType="nt:unstructured">
																	<mapping
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																		fieldDescription="Please provide mapping"
																		fieldLabel="Page Mapping"
																		name="./esMapping"/>
																				
															</items>
														</column>
													 </items>
												</field>
											</pageMapping>											   
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </page>
					<asset
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Asset Mapping"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">												
											<assetMapping
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
												composite="{Boolean}true"
												fieldLabel="Asset Metadata and Search Fields Mapping">
												<field
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/container"
													name="./esxJAssetMappingList">
													<items jcr:primaryType="nt:unstructured">
														<column
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/container">
															<items jcr:primaryType="nt:unstructured">
																	<mapping
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																		fieldDescription="Please provide mapping"
																		fieldLabel="Asset Metadata Mapping"
																		name="./esMapping"/>
																				
															</items>
														</column>
													 </items>
												</field>
											</assetMapping>											   
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </asset>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
